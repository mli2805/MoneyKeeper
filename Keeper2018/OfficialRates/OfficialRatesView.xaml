<Window x:Class="Keeper2018.OfficialRatesView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        >
    <DockPanel Margin="5">
        <DockPanel DockPanel.Dock="Bottom" Margin="5">
            <Button Margin="5" Width="100" Content="Close" DockPanel.Dock="Right"/>
            <Button Margin="5" Width="100" Content="Download" DockPanel.Dock="Right"/>
            <TextBlock Margin="5" DockPanel.Dock="Left" Text="{Binding Clock}"/>
            <TextBlock/>
        </DockPanel>
        <ListView></ListView>
    </DockPanel>
</Window>
