<Window x:Class="Keeper2018.ShellView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:keeper2018="clr-namespace:Keeper2018"
        Width="600" Height="700" WindowStartupLocation="CenterScreen">

    <DockPanel>
        <keeper2018:MainMenuView DockPanel.Dock="Top" DataContext="{Binding MainMenuViewModel}"/>
        <keeper2018:AccountTreeView DataContext="{Binding AccountTreeViewModel}"/>
    </DockPanel>

</Window>